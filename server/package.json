{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "server": "nodemon ./bin/www --exec babel-node -e js",
    "db_create": "sequelize db:create",
    "db_migrate": "sequelize db:migrate",
    "db_reset": "sequelize db:drop && sequelize db:create && sequelize db:migrate",
    "db_seed": "node db/seeders/users.js && node db/seeders/posts.js"
  },
  "dependencies": {
    "babel-register": "^6.26.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "express": "~4.16.0",
    "faker": "^4.1.0",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "mysql2": "^1.6.4",
    "pug": "2.0.0-beta11",
    "sequelize": "^4.42.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "nodemon": "^1.18.8",
    "sequelize-cli": "^5.4.0"
  }
}
